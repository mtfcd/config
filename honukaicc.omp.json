{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "plain",
          "foreground": "#FFFFFF",
          "properties": {
            "user_info_separator": " <#ffffff>@</> ",
            "prefix": "<#0377C8># </>",
            "user_color": "#0377C8",
            "host_color": "#4A9207"
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#0973C0",
          "properties": {
            "folder_separator_icon": "\\",
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#B8B80A",
          "properties": {
            "prefix": "<#ffffff>on git:</>"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
		{
		  "type": "go",
		  "style": "plain",
		  "foreground": "#0377C8",
		  "properties": {
			"prefix": "\uFCD1"
		  }
		},
		{
		  "type": "node",
		  "style": "plain",
		  "foreground": "#6CA35E",
		  "properties": {
			"prefix": "\uE718 "
		  }
		},
        {
          "type": "python",
          "style": "plain",
          "foreground": "#ffd343",
          "properties": {
            "prefix": "\uE235 "
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "#ffffff",
          "properties": {
            "prefix": "[",
            "postfix": "]"
          }
        }
      ]
    },
    {
      "type": "newline"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "foreground": "#CECE04",
          "properties": {
            "prefix": ""
          }
        },
		
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#CB4B16"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#CC4B16",
          "properties": {
            "prefix": "",
            "text": "\u279C"
          }
        }
      ]
    }
  ],
  "final_space": true
}
